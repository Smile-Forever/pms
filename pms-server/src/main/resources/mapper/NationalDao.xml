<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.tedu.pms.dao.NationalDao">
    <select id="findAll" resultType="cn.tedu.pms.entity.NationalEntity">
        select
          id ,
          name,
          enable
        from
          national
        where enable=1
    </select>
</mapper>